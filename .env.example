# Application
NODE_ENV=development
APP_PORT=3000
APP_CLUSTERING=false

# Database
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_USERNAME=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DBNAME=sportsdb

# Auth
AUTH_JWTSECRET=your_jwt_secret_key
AUTH_REFRESHTOKENSECRET=your_refresh_token_secret
AUTH_ACCESSTOKENEXPIRESIN=15m
AUTH_REFRESHTOKENEXPIRESIN=7d

# Frontend
FRONTEND_ORIGIN=http://localhost:3000

# Firebase
FIREBASE_PROJECTID=<your_firebase_project_id>
FIREBASE_PRIVATEKEY=<your_firebase_private_key>
FIREBASE_EMAIL=<your_firebase_email>

# Project
PROJECT_NAME=sports-complex-app
PROJECT_DESCRIPTION=Sports complex management application
PROJECT_VERSION=1.0.0

# Swagger
SWAGGER_USERNAME=admin
SWAGGER_PASSWORD=admin123

# Sendgrid
SENDGRID_APIKEY=your_sendgrid_api_key

# Mailer
MAILER_EMAIL=no-reply@sportscomplexapp.com

# S3
S3_ACCESSKEY=your_s3_access_key
S3_SECRET=your_s3_secret
S3_BUCKET=sports-complex-bucket
S3_REGION=us-east-1